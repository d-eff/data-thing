<div class='col-md-9 container'>
  <h3>User {{data.userId}}</h3>
  <h4>Events: (Occur in only one source)</h4>
  <table class='table table-striped table-hover'>
    <thead>
      <tr>
        <th>Event Date</th>
        <th>Event Type</th>
        <th>Source</th>
        <th>Record</th>
      </tr>
    </thead>
    <tbody>
      {{#each data.events}}
      <tr>
          <td>{{this.eventDate}}</td>
          <td>{{this.eventType}}</td>
          <td>{{this.src}}</td>
          <td>{{this.record}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>

  <h4>QA Activity</h4>
  <table class='table table-striped table-hover'>
    <thead>
      <tr>
        <th>Date</th>
        <th>Activity</th>
        <th>Billing</th>
        <th>Platform</th>
        <th>Paylod</th>
      </tr>
    </thead>
    <tbody>
    {{#each data.qaAct}}
        <tr>
          <td>{{this.dt}}</td>
          <td>{{this.activity}}</td>
          <td>{{this.billing}}</td>
          <td>{{this.platform}}</td>
          <td>{{this.payload}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

  <h4>Prod Activity</h4>
  <table class='table table-striped table-hover'>
    <thead>
      <tr>
        <th>Date</th>
        <th>Activity</th>
        <th>Billing</th>
        <th>Platform</th>
        <th>Paylod</th>
      </tr>
    </thead>
    <tbody>
    {{#each data.prodAct}}
        <tr>
          <td>{{this.dt}}</td>
          <td>{{this.activity}}</td>
          <td>{{this.billing}}</td>
          <td>{{this.platform}}</td>
          <td>{{this.payload}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>